body{
    background-color: #e7e7e7;
}

.main-container{
    background-color: #fff;
    margin: 15/16rem;
    padding: 15/16rem;
    .tab-list{
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #ebeef7;
        .tab-item{
            height: 36/16rem;
            line-height: 36/16rem;
            text-align: left;
            padding-left: 20/16rem;
            letter-spacing: 1/16rem;
            font-size: 14/16rem;
        }
    }
    .tab{
        overflow: hidden;
        width: 100%;
        .tab-content{
           height: 1000px;
            .operation-list{
                margin: 0;
                padding: 0;
                height: 48/16rem;
                position: relative;
                .operation-item{
                    float: left;
                    height: 48/16rem;
                    line-height: 48/16rem;
                    text-align: center;
                    letter-spacing: 1/16rem;
                    font-size: 14/16rem;
                    padding-left: 10/16rem;
                    padding-right: 10/16rem;
                    cursor: pointer;
                    .menu{
                        display: none;
                        cursor: default;
                        position: absolute;
                        top: 38/16rem;
                        left: 0;
                        background-color: #f0f0f0;
                        width: 100%;
                        border: solid #E0E0E0 1/16rem;
                        box-shadow: 0 3/16rem 4/16rem rgba(0,0,0,0.18);
                        z-index: 50;
                        .value{
                            float: left;
                        }
                        .arrowhead{
                            float: right;
                            font-size: 11/16rem;
                        }
                        .menu-title{
                            text-align: left;
                            margin-top: 0;
                            margin-bottom: 0;
                            margin-left: 20/16rem;
                            padding-bottom: 10/16rem;
                            font-size: 15/16rem;
                            letter-spacing: 1/16rem;
                            border-bottom: #ccc dashed 1/16rem;
                        }
                        dl{
                            margin-top: 20/16rem;
                            margin-bottom: 20/16rem;
                            margin-left: 20/16rem;
                           .condition-title{
                               float: left;
                                letter-spacing: 1/16rem;
                                height: 32/16rem;
                                line-height: 32/16rem;
                                font-weight: 600;
                                margin-right: 20/16rem;
                           } 
                           dd{
                               float: left;
                               margin-left: 0;
                               .condition-list{
                                    margin: 0;
                                    padding: 0;
                                    list-style: none;
                                    width: 800/16rem;
                                    min-width: 680/16rem;
                                    height: 171/16rem;
                                    
                                    .dropdown-list{
                                        margin: 0;
                                        padding: 0;
                                    }
                                   .label{
                                       float: left;
                                        width: 80/16rem;
                                        letter-spacing: 1/16rem;
                                        height: 32/16rem;
                                        line-height: 32/16rem;
                                        text-align: right;
                                        padding-right: 5/16rem;
                                        margin-bottom: 25/16rem;
                                        margin-left: 10/16rem;
                                    }
                                   .condition{
                                       float: left;
                                        margin-right: 40/16rem;
                                        margin-bottom: 10/16rem;
                                        height: 48/16rem;
                                        margin-left: 0;
                                        .keyword {
                                            width: 260/16rem;
                                            height: 32/16rem;
                                            padding-left: 10/16rem;
                                            padding-right: 10/16rem;
                                            margin: 0;
                                            border: solid #ccc 1/16rem;
                                            outline: none;
                                            letter-spacing: 1/16rem;
                                            position: relative;
                                            top: -8/16rem;
                                        }
                                        .import-text{
                                        	color:red;
											position:relative;
											top : -8/16rem;
                                        }
                                        .file {
                                            height: 32/16rem;
                                            padding-left: 10/16rem;
                                            padding-right: 10/16rem;
                                            margin: 0;
                                            outline: none;
                                            letter-spacing: 1/16rem;
                                    		font-size: 14/16rem;
                                    		font-weight: 600;
                                            position: relative;
                                            top: -8/16rem;
                                        }
                                    }
                               }
                               .menu-btn {
                                    display: block;
                                    width: 120/16rem;
                                    height: 32/16rem;
                                    margin-bottom: 25/16rem;
                                    margin-right: 25/16rem;
                                    background-color: #F4494E;
                                    border: none;
                                    color: #fff;
                                    outline: none;
                                    border-radius: 3/16rem;
                                    letter-spacing: 1/16rem;
                                    font-size: 14/16rem;
                                    font-weight: 600;
                                    &:active {
                                        background-color: #CD0C12;
                                    }
                                }
                            }
                            
                        }
                        
                    }
                }
                .return{
                    float: right;
                    height: 48/16rem;
                    line-height: 48/16rem;
                    text-align: center;
                    letter-spacing: 1/16rem;
                    font-size: 14/16rem;
                    padding-left: 10/16rem;
                    padding-right: 30/16rem;
                    cursor: pointer;
                    color: #0066FF;
                }
            }
            
           .fixed-table{
                .click{
                    cursor: pointer;
                    color: #0066FF;
                    &:active{
                        color: #2EA8FF;
                    }
                    &:visited{
                        color:#9933ff;
                    }
                }
            }
            .page-list{
                margin: 0;
                padding: 0;
                overflow: hidden;
                background-color: #EBEEF7;
                margin-top: 15/16rem;
                .page-item{
                    height: 40/16rem;
                    font-size: 14/16rem;
                    line-height: 40/16rem;
                }
                .page-info{
                    float: left;
                    margin-left: 15/16rem;
                    span{
                        margin-left: 5/16rem;
                        margin-right: 5/16rem;
                    }
                }
                .page-operation{
                    float: right;
                    margin-right: 10/16rem;
                    a{
                        cursor: pointer;
                        padding-left: 5/16rem;
                        padding-right: 5/16rem;
                        &:hover{
                            color: #ff6700;
                        }
                    }
                    .page-active{
                        color: red;
                        font-weight: 600;
                    }
                    .page-disable{
                        color: #B9B9B9;
                        cursor: default;
                        &:hover{
                            color: #B9B9B9;
                        }
                    }
                }
            }
        
        }
    }
    .error{
        color: red !important;
    }
    
}
