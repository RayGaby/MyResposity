body {
  background-color: #e7e7e7;
}
.main-container {
  background-color: #fff;
  margin: 0.9375rem;
  padding: 0.9375rem;
}
.main-container .tab-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #EBEEF7;
}
.main-container .tab-list .tab-item {
  float: left;
  height: 2.25rem;
  line-height: 2.25rem;
  text-align: center;
  padding-left: 1.25rem;
  padding-right: 1.25rem;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  cursor: pointer;
}
.main-container .tab-list .tab-active {
  color: #fff;
  background-color: #02A0E9;
}
.main-container .tab-container {
  overflow: hidden;
}
.main-container .tab-container .tab-content {
  display: none;
}
.main-container .tab-container .tab-content .operation-list {
  margin: 0;
  padding: 0;
  height: 3rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item {
  float: left;
  height: 3rem;
  line-height: 3rem;
  text-align: center;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  padding-left: 0.625rem;
  padding-right: 0.625rem;
  cursor: pointer;
  display: none;
}
.main-container .tab-container .content-active {
  display: block;
}
.main-container .tab-container .page-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #EBEEF7;
  margin-top: 0.9375rem;
}
.main-container .tab-container .page-list .page-item {
  height: 2.5rem;
  font-size: 0.875rem;
  line-height: 2.5rem;
}
.main-container .tab-container .page-list .page-info {
  float: left;
  margin-left: 0.9375rem;
}
.main-container .tab-container .page-list .page-info span {
  margin-left: 0.3125rem;
  margin-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation {
  float: right;
  margin-right: 0.625rem;
}
.main-container .tab-container .page-list .page-operation a {
  cursor: pointer;
  padding-left: 0.3125rem;
  padding-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation a:hover {
  color: #ff6700;
}
.main-container .tab-container .page-list .page-operation .page-active {
  color: red;
  font-weight: 600;
}
.main-container .tab-container .page-list .page-operation .page-disable {
  color: #B9B9B9;
  cursor: default;
}
.main-container .tab-container .page-list .page-operation .page-disable:hover {
  color: #B9B9B9;
}
