body {
  background-color: #e7e7e7;
}
.main-container {
  background-color: #fff;
  margin: 0.9375rem;
  padding: 0.9375rem;
}
.main-container .tab-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #EBEEF7;
}
.main-container .tab-list .tab-item {
  color: #fff;
  background-color: #02A0E9;
  float: left;
  height: 2.25rem;
  line-height: 2.25rem;
  text-align: center;
  padding-left: 1.25rem;
  padding-right: 1.25rem;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  cursor: pointer;
}
.main-container .tab-container .tab-content {
  position: relative;
}
.main-container .tab-container .tab-content .operation-list {
  margin: 0;
  padding: 0;
  height: 3rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item {
  float: left;
  height: 3rem;
  line-height: 3rem;
  text-align: center;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  padding-left: 0.625rem;
  padding-right: 0.625rem;
  cursor: pointer;
}
.main-container .tab-container .tab-content .update-container {
  display: none;
  position: absolute;
  background-color: #F0F0F0;
  border-radius: 0.3125rem;
  width: 100.20rem;
}
.main-container .tab-container .tab-content .update-container .title {
  padding-top: 0.9375rem;
  padding-left: 1.875rem;
  padding-right: 1.875rem;
}
.main-container .tab-container .tab-content .update-container .title .first-title {
  font-size: 1rem;
  border-bottom: dashed 0.09375rem #999;
  padding-bottom: 0.9375rem;
}
.main-container .tab-container .tab-content .update-container .second-title {
  width: 6.25rem;
  padding-left: 1.875rem;
  padding-top: 0.9375rem;
  float: left;
  font-size: 0.875rem;
}
.main-container .tab-container .tab-content .update-container .update-content {
  padding-top: 0.9375rem;
  padding-left: 8.125rem;
  padding-bottom: 1.875rem;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item {
  padding-top: 1.25rem;
  padding-bottom: 0.625rem;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item label {
  width: 4.6875rem;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  margin-right: 0.625rem;
  display: inline-block;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item .input {
  width: 11.25rem;
  height: 1.875rem;
  outline: none;
  border-radius: 0.1875rem;
  border: solid 0.0625rem #e0e0e0;
  color: #333;
  font-size: 0.8125rem;
  padding-left: 0.9375rem;
  padding-right: 0.9375rem;
  margin-right: 1.875rem;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item .btn {
  width: 7.5rem;
  height: 1.875rem;
  border-radius: 0.1875rem;
  border: none;
  font-weight: bold;
  font-size: 0.875rem;
  outline: none;
  cursor: pointer;
  margin-right: 1.25rem;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item .red {
  color: #fff;
  background-color: #E86167;
}
.main-container .tab-container .tab-content .update-container .update-content .update-item .red:active {
  background-color: #EB777C;
}
.main-container .tab-container .page-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #EBEEF7;
  margin-top: 0.9375rem;
}
.main-container .tab-container .page-list .page-item {
  height: 2.5rem;
  font-size: 0.875rem;
  line-height: 2.5rem;
}
.main-container .tab-container .page-list .page-info {
  float: left;
  margin-left: 0.9375rem;
}
.main-container .tab-container .page-list .page-info span {
  margin-left: 0.3125rem;
  margin-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation {
  float: right;
  margin-right: 0.625rem;
}
.main-container .tab-container .page-list .page-operation a {
  cursor: pointer;
  padding-left: 0.3125rem;
  padding-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation a:hover {
  color: #ff6700;
}
.main-container .tab-container .page-list .page-operation .page-active {
  color: red;
  font-weight: 600;
}
.main-container .tab-container .page-list .page-operation .page-disable {
  color: #B9B9B9;
  cursor: default;
}
.main-container .tab-container .page-list .page-operation .page-disable:hover {
  color: #B9B9B9;
}
